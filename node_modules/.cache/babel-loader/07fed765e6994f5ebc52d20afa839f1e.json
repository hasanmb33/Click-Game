\n\t\t\tsetTimeout(() =>{\r\n\t\t\t\tthis.clickTime = 2000/num\r\n\t\t\t\tthis.x = (170+Math.random()*500).toString()\r\n\t\t\t\tthis.y = (50+Math.random()*750).toString()\r\n\t\t\t\tconsole.log(\"time: \" +this.clickTime)\r\n\t\t\t\tthis.showSquar = true\r\n\t\t\t\tthis.startTimer()\r\n\t\t\t}, 2000)\r\n\t\t}\r\n\t}\r\n}\t\r\n</script>\r\n\r\n\r\n\r\n<style>\r\n\t#squar{\r\n    width: 100px;\r\n    height: 100px;\r\n    border-radius: 5px;\r\n    background: rgb(0,70,40,0.8);\r\n    color: white;\r\n    text-align: center;\r\n    vertical-align: middle;\r\n    line-height: 100px; \r\n    position: absolute; \r\n    \r\n  \t}\r\n</style>"],"sourceRoot":""}]}